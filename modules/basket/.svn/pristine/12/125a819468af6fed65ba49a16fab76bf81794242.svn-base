 input[type="radio"]{	background-color: #93D1E4;}.listingbasket .col td  input[type="radio"]{	background-color: #DEEDF3;}.listingbasket td  input{	width: 200px;}/*Automplete*/div.autocomplete {  position: absolute;  width: 500px;  background-color: white;  border: 1px solid #888;  margin: 0px;  padding: 0px;}div.autocomplete ul {  list-style-type: none;  margin: 0px;  padding: 0px;  max-height: 20em;  overflow: auto;}div.autocomplete ul li.selected {    background-color: #ffb;}div.autocomplete ul li {  /*background-color: #DEEDF3;*/  list-style-type:none;  display: block;  margin: 0;  padding: 2px;  cursor: pointer;  text-align: left;}div.autocomplete ul li span.informal {    color: grey;}.basket_elem{	margin-left:10px;}#manage_groupbasket li{	text-align: left;}